{
  "scripts": {
    "dev": "node src/app.js",
    "serve": "nodemon serve",
    "start": "nodemon src/app.js -e html,js,njk,css",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\" && prettier --write \"**/*.njk\" --config .prettierrc-nunjucks",
    "precommit": "npm run prettier && git add ."
  },
  "dependencies": {
    "@govuk-prototype-kit/common-templates": "1.2.2",
    "express": "^4.18.2",
    "govuk-frontend": "^4.7.0",
    "govuk-prototype-kit": "13.13.6",
    "path": "^0.12.7"
  },
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-html": "^7.1.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "nunjucks": "^3.2.4",
    "nunjucks-loader": "^3.0.0",
    "prettier": "^3.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
